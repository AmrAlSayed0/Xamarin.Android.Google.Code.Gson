<Project Sdk="MSBuild.Sdk.Extras/3.0.23" >
    <PropertyGroup>
        <OutputType>Library</OutputType>
        <IsBindingProject>true</IsBindingProject>
        <RootNamespace>Xamarin.Google.Code.Gson</RootNamespace>
        <AssemblyName>Xamarin.Google.Code.Gson</AssemblyName>
        <Product>$(AssemblyName) ($(TargetFramework))</Product>
        <AssemblyVersion>1.0.0.0</AssemblyVersion>
        <AssemblyFileVersion>1.0.0.0</AssemblyFileVersion>
        <Version>1.0.0.0</Version>
        <NeutralLanguage>en</NeutralLanguage>
        <FileAlignment>512</FileAlignment>
        <Deterministic>true</Deterministic>
        <LangVersion>9.0</LangVersion>
        <Nullable>enable</Nullable>
        <_EnableInterfaceMembers>true</_EnableInterfaceMembers>
        <ErrorReport>prompt</ErrorReport>
        <WarningLevel>4</WarningLevel>
        <TargetFrameworks>monoandroid9.0;monoandroid10.0;monoandroid11.0</TargetFrameworks>
        <AndroidUseIntermediateDesignerFile>true</AndroidUseIntermediateDesignerFile>
        <MonoAndroidResourcePrefix>Resources</MonoAndroidResourcePrefix>
        <MonoAndroidAssetsPrefix>Assets</MonoAndroidAssetsPrefix>
        <AndroidResgenFile>Resources\Resource.designer.cs</AndroidResgenFile>
        <EnableProguard>false</EnableProguard>
        <AndroidEnableMultiDex>true</AndroidEnableMultiDex>
        <AndroidUseAapt2>true</AndroidUseAapt2>
        <AndroidDexTool>d8</AndroidDexTool>
        <AndroidLinkTool>r8</AndroidLinkTool>
        <AndroidClassParser>class-parse</AndroidClassParser>
        <AndroidCodegenTarget>XAJavaInterop1</AndroidCodegenTarget>
        <RestoreProjectStyle>PackageReference</RestoreProjectStyle>
        <DebugType>portable</DebugType>
        <DefineConstants>$(DefineConstants);TRACE;_BINDING_;</DefineConstants>
    </PropertyGroup>
    <PropertyGroup>
        <PackageId>Xamarin.Android.Google.Code.Gson</PackageId>
        <Title>A Xamarin binding for Google's Android library "Gson"</Title>
        <Summary>A Xamarin binding for Google's Android library "Gson"</Summary>
        <Description>A Xamarin binding for Google's Android library "Gson"</Description>
        <Authors>AmrAlSayed0</Authors>
        <Owners>AmrAlSayed0</Owners>
        <Copyright>© AmrAlSayed0. All rights reserved.</Copyright>
        <PackageTags>google gson code xamarin android monodroid</PackageTags>
        <PackageLicenseExpression>MIT</PackageLicenseExpression>
        <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
        <PackageProjectUrl>https://github.com/AmrAlSayed0/Xamarin.Android.Google.Code.Gson</PackageProjectUrl>
        <RepositoryUrl>https://github.com/AmrAlSayed0/Xamarin.Android.Google.Code.Gson</RepositoryUrl>
        <PackageVersion>2.8.6.0</PackageVersion>
        <RepositoryType>git</RepositoryType>
        <AllowedOutputExtensionsInPackageBuildOutputFolder>$(AllowedOutputExtensionsInPackageBuildOutputFolder);.pdb</AllowedOutputExtensionsInPackageBuildOutputFolder>
    </PropertyGroup>
    <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Debug|AnyCPU' " >
        <DebugSymbols>true</DebugSymbols>
        <Optimize>false</Optimize>
        <OutputPath>bin\Debug\</OutputPath>
        <DefineConstants>$(DefineConstants);DEBUG;</DefineConstants>
    </PropertyGroup>
    <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' " >
        <DebugSymbols>false</DebugSymbols>
        <Optimize>true</Optimize>
        <OutputPath>bin\Release\</OutputPath>
        <DefineConstants>$(DefineConstants);RELEASE;</DefineConstants>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    </PropertyGroup>
    <ItemGroup>
        <Reference Include="Mono.Android" />
        <Reference Include="System" />
    </ItemGroup>
    <ItemGroup>
        <Folder Include="Additions\" />
        <Folder Include="Jars\" />
        <Folder Include="Transforms\" />
    </ItemGroup>
    <ItemGroup>
        <EmbeddedJar Include="Jars\gson-2.8.6.jar" />
        <None Include="Jars\gson-2.8.6.jar"
              Pack="True"
              PackagePath="jar\gson-2.8.6.jar" />
    </ItemGroup>
    <ItemGroup>
        <None Remove="Transforms\*.xml" />
        <TransformFile Include="Transforms\*.xml" />
    </ItemGroup>
</Project>
